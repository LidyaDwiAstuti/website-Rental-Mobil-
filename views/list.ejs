<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <!-- css Boostrap -->
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet"
        integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">
    <!-- icon -->
    <link href='https://unpkg.com/boxicons@2.0.7/css/boxicons.min.css' rel='stylesheet'>
    <!-- css w3school -->
    <link rel="stylesheet" href="https://www.w3schools.com/w3css/4/w3.css">
    <link rel="stylesheet" href="../css/list.css">
</head>

<body>
    <%- include('./partial/up') %>
    <!-- Start Sidebar -->
    <div id="startSidebar">
        <div class="class-sidebar w3-sidebar w3-bar-block ">
            <div class="d-grid p-1 gap-2">
                <a href="#" class="w3-button">
                    <img src="./img/Rectangle_63.png" alt="">
                </a>
            </div>

            <ul class="nav flex-column text-center p-1">
                <li class="nav-item">
                    <div class="d-grid gap-2">
                        <a href="#" class="w3-button ps-0 pe-0">
                            <img src="./img/Group_2.png" alt="">
                            <p>Dashboard</p>
                        </a>
                    </div>
                </li>
                <li class="nav-item">
                    <div class="d-grid gap-2">
                        <a href="#" class="w3-button">
                            <img src="./img/fi_truck.png" alt="">
                            <p>Cars</p>
                        </a>
                    </div>
                </li>
            </ul>
        </div>
    </div>
    <!-- End Sidebar -->
    <!-- Start Content -->
    <div id="startcontent">
        <!-- Start Content Navbar top -->
        <nav class="navbar navbar-expand-lg navbar-light bg-white sticky-top">
            <div class="container-fluid ">
                <a class="navbar-brand" href="#"> <svg width="100" height="34" viewBox="0 0 100 34" fill="none"
                        xmlns="http://www.w3.org/2000/svg">
                        <rect width="100" height="34" fill="#CFD4ED" />
                    </svg></a>
                <button class="navbar-toggler" type="button" data-bs-toggle="collapse"
                    data-bs-target="#navbarTogglerDemo02" aria-controls="navbarTogglerDemo02" aria-expanded="false"
                    aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div class="collapse navbar-collapse" id="navbarTogglerDemo02">
                    <ul class="navbar-nav me-auto mb-2 mb-lg-0">
                        <li class="nav-item">
                            <a class="nav-link active" aria-current="page" href="#"> <i class='bx bx-menu'
                                    style="padding: 15px;"></i></a>
                        </li>
                    </ul>

                    <div class="row">
                        <div class="col d-flex">
                            <input type="search" placeholder="Search" name="fname"
                                class="form-control searchcontent me-2">
                            <button class="btn btn-outline-primary" type="submit">Search</button>
                            <img class="ps-2" src="./img/Group_15.png" alt="">
                            <a href="#" style="width: 50%;" class=" w3-button">Unis Badri</a>
                            <a href="" class="down-icon"><i class='bx bxs-chevron-down'></i></a>
                        </div>
                    </div>
                </div>
            </div>
        </nav>
        <!-- End Content Navbar top -->

        <!-- Start content sidebar -->
        <div class="content-sidebar w3-sidebar w3-bar-block bg-white pt-3">
            <a href="#" class="w3-bar-item w3-button">
                <strong>CARS</strong>
            </a>
            <a href="#" class="w3-bar-item w3-button mt-3">
                <strong>List Car</strong>
            </a>
        </div>
        <!-- End content sidebar -->

        <!-- Start bagian content -->
        <div class="content1 w3-container bg-light">
            <!-- Start Menu Content -->
            <div class="container-fluid">
                <div class="row">
                    <div class="col p-3">
                        <strong>Cars > </strong> <a href="/"> List Car </a> > Add New Car
                    </div>
                </div>
                <div class="row">
                    <div class="col p-3">
                        <strong>
                            Add New Car
                        </strong>
                    </div>
                </div>
            </div>
            <!-- End Menu Content -->
            <!-- Start Card Content -->
            <form action="/api/cars" method="post">
                <div class="container-fluid">
                    <div class="row card-isi-kontent">
                        <div class="col">
                            <div class="card">
                                <div class="card-body">
                                    <div class="row">
                                        <div class="col">
                                            <div class="mb-3 row">
                                                <label class="col-sm-2 col-form-label">Nama*</label>
                                                <div class="col-4">
                                                    <input type="text" placeholder="Avanza" class="form-control"
                                                        name="name">
                                                </div>
                                            </div>
                                            <div class="mb-3 row">
                                                <label class="col-sm-2 col-form-label">Sewa Per
                                                    Hari*</label>
                                                <div class="col-4">
                                                    <input type="text" placeholder="Rp 0,-" class="form-control"
                                                        name="harga">
                                                </div>
                                            </div>
                                            <div class="mb-3 row">
                                                <label class="col-sm-2 col-form-label">Ukuran*</label>
                                                <div class="col-4">
                                                    <select class="form-select" aria-label="Default select example"
                                                        name="id_size">
                                                        <option value="" disabled selected hidden>Pilih Tipe Driver
                                                        </option>
                                                        <% for (var i in size) { %>
                                                        <option value="<%= size[i].id %>"><%= size[i].name %>
                                                        </option>
                                                        <% } %>
                                                    </select>
                                                </div>
                                            </div>
                                            <div class="mb-3 row">
                                                <label class="col-sm-2 col-form-label">Foto*</label>
                                                <div class="col-4">
                                                    <div class="custom-file-upload">
                                                        <input type="file" class="form-control custom-upload"
                                                            name="url_image">
                                                    </div>
                                                    <small class="text-muted">File size max.
                                                        2MB</small>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col">
                            <div class="d-grid gap-2 d-md-block">
                                <button class="btn btn-outline-primary" type="button">Cancel</button>
                                <button class="btn btn-primary" id="toastbtn" type="button submit">Save</button>
                            </div>
                        </div>
                    </div>
                </div>
            </form>
            <!-- End bagian content -->

        </div>
        <!-- End Content -->
        <!-- js -->
        <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js"
            integrity="sha384-ka7Sk0Gln4gmtz2MlQnikT1wXgYsOg+OMhuP+IlRH9sENBO0LRn5q+8nbTov4+1p" crossorigin="anonymous">
        </script>
</body>

</html>