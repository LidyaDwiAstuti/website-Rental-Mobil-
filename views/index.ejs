<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Challenge 5 Binar</title>
    <!-- css Boostrap -->
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet"
        integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">
    <!-- icon -->
    <link href='https://unpkg.com/boxicons@2.0.7/css/boxicons.min.css' rel='stylesheet'>
    <!-- css w3school -->
    <link rel="stylesheet" href="https://www.w3schools.com/w3css/4/w3.css">
    <link rel="stylesheet" href="./css/style.css">
</head>

<body>
    <%- include('./partial/pop') %>
    <%- include('./partial/popup') %>

    <!-- Start Sidebar -->
    <div id="startSidebar">
        <div class="class-sidebar w3-sidebar w3-bar-block ">
            <div class="d-grid p-1 gap-2">
                <a href="#" class="w3-button">
                    <img src="./img/Rectangle_63.png" alt="">
                </a>
            </div>

            <ul class="nav flex-column text-center p-1">
                <li class="nav-item">
                    <div class="d-grid gap-2">
                        <a href="#" class="w3-button ps-0 pe-0">
                            <img src="./img/Group_2.png" alt="">
                            <p>Dashboard</p>
                        </a>
                    </div>
                </li>
                <li class="nav-item">
                    <div class="d-grid gap-2">
                        <a href="#" class="w3-button">
                            <img src="./img/fi_truck.png" alt="">
                            <p>Cars</p>
                        </a>
                    </div>
                </li>
            </ul>
        </div>
    </div>
    <!-- End Sidebar -->
    <!-- Start Content -->
    <div id="startcontent">
        <!-- Start Content Navbar top -->
        <nav class="navbar navbar-expand-lg navbar-light bg-white sticky-top">
            <div class="container-fluid ">
                <a class="navbar-brand" href="#"> <svg width="100" height="34" viewBox="0 0 100 34" fill="none"
                        xmlns="http://www.w3.org/2000/svg">
                        <rect width="100" height="34" fill="#CFD4ED" />
                    </svg></a>
                <button class="navbar-toggler" type="button" data-bs-toggle="collapse"
                    data-bs-target="#navbarTogglerDemo02" aria-controls="navbarTogglerDemo02" aria-expanded="false"
                    aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div class="collapse navbar-collapse" id="navbarTogglerDemo02">
                    <ul class="navbar-nav me-auto mb-2 mb-lg-0">
                        <li class="nav-item">
                            <a class="nav-link active" aria-current="page" href="#"> <i class='bx bx-menu'
                                    style="padding: 15px;"></i></a>
                        </li>
                    </ul>

                    <div class="row">
                        <div class="col d-flex">
                            <input type="search" placeholder="Search" name="fname"
                                class="form-control searchcontent me-2">
                            <button class="btn btn-outline-primary" type="submit">Search</button>
                            <img class="ps-2" src="./img/Group_15.png" alt="">
                            <a href="#" style="width: 50%;" class=" w3-button">Unis Badri</a>
                            <a href="" class="down-icon"><i class='bx bxs-chevron-down'></i></a>
                        </div>
                    </div>
                </div>
            </div>
        </nav>
        <!-- End Content Navbar top -->

        <!-- Start content sidebar -->
        <div class="content-sidebar w3-sidebar w3-bar-block bg-white pt-3">
            <a href="#" class="w3-bar-item w3-button">
                <strong>CARS</strong>
            </a>
            <a href="#" class="w3-bar-item w3-button mt-3">
                <strong>List Car</strong>
            </a>
        </div>
        <!-- End content sidebar -->

        <!-- Start bagian content -->
        <div class="content1 w3-container bg-light">
            <!-- Start Menu isi Content -->
            <div class="container-fluid">
                <div class="row">
                    <div class="col p-3">
                        <strong>Cars > </strong> List Car
                    </div>
                </div>
                <div class="row">
                    <div class="col p-3">
                        <strong>
                            <p>List Car</p>
                        </strong>
                    </div>
                    <div class="col text-end">
                        <a href="/list" type="button" class="btn btn-primary add-new-car"><i class='bx bx-plus'></i>
                            Add New Car</a>
                    </div>
                </div>
                <div class="row">
                    <div class="col p-3">
                        <button type="button" class="btn-ukuran btn btn-primary">All</button>
                        <button type="button" class="btn-ukuran btn btn-primary disabled">Small</button>
                        <button type="button" class="btn-ukuran btn btn-primary disabled">Medium</button>
                        <button type="button" class="btn-ukuran btn btn-primary disabled">Large</button>
                    </div>
                </div>
            </div>
            <!-- End Menu isi Content -->
            <!-- Start Card Content -->
            <div class="container-fluid">
                <div class="row pt-4">
                    <% for (var i in Cars) { %>
                    <div class="col-4">
                        <!-- Start Card -->
                        <div class="card">
                            <img src="<%= Cars[i].url_image%>" class="card-img-top" alt="...">
                            <div class="card-body">
                                <p><%= Cars[i].name %>/Tipe Mobil</p>
                                <p><strong> Rp.<%= Cars[i].harga %> / hari</strong></p>
                                <p>
                                    <ul class="m-0 p-0">
                                        <li><img src="./img/fi_clock.png" alt="">
                                            <small class="text-muted">Updated at 4 Apr 2022,
                                                09.00</small>
                                        </li>
                                    </ul>
                                </p>
                                <div class="row">
                                    <div class="col">
                                        <div class="d-grid gap-2 d-block">
                                            <button class="btn btn-outline-danger deleteButton"
                                                data-id-car="<%= Cars[i].id %>" type="button" data-bs-toggle="modal"
                                                data-bs-target="#staticBackdrop"><i
                                                    class='bx bx-trash-alt'></i>Delete</button>
                                        </div>
                                    </div>
                                    <div class="col">
                                        <div class="d-grid gap-2">
                                            <a href="/update/<%= Cars[i].id%>" class="btn-edit btn btn-success"
                                                type="button"><i class='bx bx-edit'></i>Edit</a>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <!-- End Card -->
                    </div>
                    <% } %>

                    <!-- End Card 2 -->
                </div>
                <!--End Card Content -->
            </div>
            <!-- End bagian content -->

        </div>
        <!-- End Content -->
        <!-- js -->
        <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js"
            integrity="sha384-ka7Sk0Gln4gmtz2MlQnikT1wXgYsOg+OMhuP+IlRH9sENBO0LRn5q+8nbTov4+1p" crossorigin="anonymous">
        </script>
        <script src="./js/index.js"></script>
</body>

</html>